{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "output": {
        "type": "string",
        "format": "binary",
        "description": "The PDF split file content as binary stream"
      },
      "attributes": {
        "type": "object",
        "properties": {
          "pageCount": {
            "type": "integer",
            "description": "Number of pages in this split PDF"
          },
          "fileSize": {
            "type": "integer",
            "description": "Size of this split PDF in bytes"
          }
        }
      }
    }
  }
}
